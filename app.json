{
  "expo": {
    "name": "PackSmart Travel Planner",
    "slug": "PackSmart Travel Planner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon-whg.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/app-icon-whg.png",
      "resizeMode": "contain",
      "backgroundColor": "#0EA5E9"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.packsmart.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon-whg.png",
        "backgroundColor": "#0EA5E9"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.packsmart.app"
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "PackSmart Travel Planner",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "backendUrl": "https://pka9s4bqakktts6r3382fy67v95ajj8g.app.specular.dev"
    }
  }
}